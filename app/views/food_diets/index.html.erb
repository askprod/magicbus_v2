<p id="notice"><%= notice %></p>

<h1>Food Diets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_diets.each do |food_diet| %>
      <tr>
        <td><%= link_to 'Show', food_diet %></td>
        <td><%= link_to 'Edit', edit_food_diet_path(food_diet) %></td>
        <td><%= link_to 'Destroy', food_diet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food Diet', new_food_diet_path %>
